{
    "generator": "retain_lines",

    "bundle": {
      "modules_identifier": "__DARKLUA_BUNDLE_MODULES",
      "excludes": [],
  
      "require_mode": {
        "name": "path",
  
        "module_folder_name": "init",
  
        "sources": {
          "pkg": "./Packages"
        }
      }
    },
  
    "rules": [
      "remove_comments",
      "remove_spaces",
      {
        "rule": "inject_global_value",
        "identifier": "DEBUG",
        "value": false
      },
      "remove_nil_declaration",
      "compute_expression",
      "remove_unused_if_branch",
      "filter_after_early_return",
      "remove_empty_do"
    ]
  }